generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "windows"]
  output   = "./generated/client_publications"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_PUBLICATIONS")
}

model Papers {
  id          Int     @id @default(autoincrement()) @db.UnsignedSmallInt
  author1     String? @db.TinyText
  all_authors String? @db.Text
  year        Int?    @db.Year
  title       String? @db.Text
  journal     String? @db.Text
  volume      Int?    @db.UnsignedSmallInt
  issue       Int?    @db.UnsignedTinyInt
  first_page  String? @db.TinyText
  last_page   String? @db.TinyText
  link        String? @db.Text
}
